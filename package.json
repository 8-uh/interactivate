{
  "name": "interactivate",
  "id": "interactivate",
  "version": "0.0.4",
  "description": "Interactive code editing in browser",
  "keywords": [
    "interactivate"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/interactivate",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/interactivate.git",
    "web": "https://github.com/Gozala/interactivate"
  },
  "bugs": {
    "url": "http://github.com/Gozala/interactivate/issues/"
  },
  "devDependencies": {
    "test": "~0.x.0",
    "repl-utils": "~1.0.0",
    "phantomify": "~0.x.0",
    "browserify": "~1.x.0"
  },
  "main": "./interactivate.js",
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
    "test-browser": "node ./node_modules/phantomify/bin/cmd.js ./test/index.js",
    "test-node": "node ./test/index.js",
    "repl": "node node_modules/repl-utils",
    "start": "node ./node_modules/browserify/bin/cmd.js --exports require --watch --debug --entry ./main.js --outfile ./build/main.js"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Gozala/interactivate/License.md"
    }
  ],
  "dependencies": {
    "method": "~1.0.1",
    "diffpatcher": "~1.0.0"
  }
}
